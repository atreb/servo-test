{
  "name": "servo-test",
  "version": "0.0.1",
  "description": "Simple Express app for servo testing",
  "main": "server.js",
  "scripts": {
    "test": "echo success",
    "start": "node server.js",
    "dev": "supervisor -e html,js,json -w . server.js",
